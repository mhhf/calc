  "rules" : {
    "RuleZer" : {
      "Id" : ["-- : A?A |- -- : A?A",    ""]
    },
    "RuleCut" : {
      "SingleCut" : ["?X, ?Z |- ?Y", "?X |- -- : F?A",  "?Z, -- : F?A |- ?Y"]
    },
    "RuleStruct": {
      "P_L" :    [
        "(?X1, ?X2), (?Y1, ?Y2) |- ?Z",
        "(?X1, ?Y1), (?X2, ?Y2) |- ?Z"
      ],
      "P_R" :    [
        "?Z |- (?Y1, ?Y2), (?X1, ?X2)",
        "?Z |- (?Y1, ?X1), (?Y2, ?X2)"
      ],
      "A_R" :    [
        "?X |- ?Y1 , (?Y2 , ?Y3)",
        "?X |- (?Y1 , ?Y2) , ?Y3"
      ],
      "A_R2" :    [
        "?X |- (?Y1 , ?Y2) , ?Y3",
        "?X |- ?Y1 , (?Y2 , ?Y3)"
      ],
      "A_L" :    [
        "?Y1 , (?Y2 , ?Y3) |- ?X",
        "(?Y1 , ?Y2) , ?Y3 |- ?X"
      ],
      "A_L2" :    [
        "(?Y1 , ?Y2) , ?Y3 |- ?X",
        "?Y1 , (?Y2 , ?Y3) |- ?X"
      ],
      "I_L_L" :   [
        "?X |- ?Y",
        "I, ?X |- ?Y"
      ],
      "I_L_L2" :   [
        "I, ?X |- ?Y",
        "?X |- ?Y"
      ],
      "I_L_R" :   [
        "?X |- ?Y",
        "?X, I |- ?Y"
      ],
      "I_L_R2" :   [
        "?X, I |- ?Y",
        "?X |- ?Y"
      ],
      "I_R_L" :   [
        "?X |- ?Y",
        "?X |- I, ?Y"
      ],
      "I_R_L2" :   [
        "?X |- I, ?Y",
        "?X |- ?Y"
      ],
      "I_R_R" :   [
        "?X |- ?Y",
        "?X |- ?Y, I"
      ],
      "I_R_R2" :   [
        "?X |- ?Y, I",
        "?X |- ?Y"
      ]
    },
    "RuleU" : {
      "Tensor_L" :    [
        "?X, -- : F?A * F?B |- -- : F?C",
        "?X, (-- : F?A, -- : F?B) |- -- : F?C"
      ],
      "Loli_R" :   [
        "?X |- -- : F?A -o F?B",
        "?X, -- : F?A |- -- : F?B"
      ],
      "With_L" : [
        "?X, -- : F?A & F?B |- -- : F?C",
        "?X, -- : F?A |- -- : F?C"
      ],
      "With_L2" : [
        "?X, -- : F?A & F?B |- -- : F?C",
        "?X, -- : F?B |- -- : F?C"
      ],
      "Monad_R" : [
        "?X |- -- : { F?A }",
        "?X |- -- : F?A lax"
      ],
      "Bang_R" : [
        "I |- -- : ! F?A",
        "I |- -- : F?A"
      ],
      "Bang_L" : [
        "?X, -- : ! F?A |- -- : F?B",
        "?X |- -- : F?B"
      ],
      "Forall_L" : [
        "?X, -- : forall AT?A : A?B. F?C |- -- : F?D",
        "?X, -- : F?C |- -- : F?D"
      ],
      "Forall_R" : [
        "?X |- -- : forall AT?A : A?B. F?C",
        "?X |- -- : F?C"
      ]
    },
    "RuleBin" : {
      "Tensor_R" :   [
        "?X, ?Y |- -- : F?A * F?B",
        "?X |- -- : F?A",
        "?Y |- -- : F?B"
      ],
      "Loli_L": [
        "?X, ?Y, -- : F?A -o F?B |- -- : F?C",
        "?X |- -- : F?A",
        "?Y, -- : F?B |- -- : F?C"
      ],
      "With_R": [
        "?X |- -- : F?A & F?B",
        "?X |- -- : F?A",
        "?X |- -- : F?B"
      ]
    }
  }
}
