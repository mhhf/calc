{
  "name": "calc",
  "version": "2.0.0",
  "description": "Linear logic proof calculus sandbox",
  "main": "index.js",
  "scripts": {
    "test": "node --test tests/hash.js tests/tree-sitter.js tests/calculus.test.js tests/sequent.test.js tests/kernel-ops.test.js tests/focusing.test.js tests/sequent-ext.test.js tests/focused-prover.test.js tests/api.test.js tests/ast.test.js tests/prover.test.js tests/rule-interpreter.test.js tests/manual-prover-logic.test.js tests/manual-proof-flow.test.js tests/rules2-parser.test.js tests/kernel.test.js tests/generic-prover.test.js tests/manual-api.test.js tests/ui-flow.test.js tests/store-quantifiers.test.js tests/quantifiers.test.js",
    "test:engine": "node --test tests/engine/convert.test.js tests/engine/forward.test.js tests/engine/e2e.test.js tests/engine/explore.test.js tests/engine/primitives.test.js",
    "test:ts": "node --test tests/tree-sitter.js",
    "test:e2e": "node tests/e2e-solidjs.js",
    "test:all": "npm test && npm run test:engine",
    "build:ts": "cd lib/tree-sitter-mde && tree-sitter generate && tree-sitter build --output mde.so",
    "build:ts:wasm": "cd lib/tree-sitter-mde && tree-sitter build --wasm",
    "build:bundle": "node libexec/calc-bundle",
    "dev": "npm run build:bundle && vite --config src/ui/vite.config.ts",
    "build:ui": "npm run build:bundle && vite build --config src/ui/vite.config.ts",
    "start": "node server.js",
    "preview": "npm run build:ui && node server.js",
    "bench": "node --expose-gc benchmarks/run.js",
    "bench:proof": "node --expose-gc benchmarks/run.js --category=proof",
    "bench:micro": "node --expose-gc benchmarks/run.js --category=micro",
    "bench:save": "node --expose-gc benchmarks/run.js --save",
    "bench:compare": "node --expose-gc benchmarks/run.js --compare",
    "bench:compare:all": "node --expose-gc benchmarks/compare-all.js",
    "bench:compare:all:save": "node --expose-gc benchmarks/compare-all.js --save",
    "bench:engine": "node --expose-gc benchmarks/mde/run.js",
    "bench:engine:save": "node --expose-gc benchmarks/mde/run.js --save",
    "bench:engine:compare": "node --expose-gc benchmarks/mde/run.js --compare",
    "bench:engine:stress": "node --expose-gc benchmarks/mde/run.js --stress",
    "profile": "CALC_PROFILE=1 node",
    "profile:engine": "node --expose-gc benchmarks/mde/run.js --profile",
    "profile:multisig": "CALC_PROFILE=1 node benchmarks/engine/multisig-profile-deep.js",
    "bench:multisig": "node benchmarks/engine/multisig-bench.js",
    "bench:symexec": "node benchmarks/engine/symexec-bench.js",
    "bench:symexec:save": "node benchmarks/engine/symexec-bench.js --save",
    "bench:symexec:compare": "node benchmarks/engine/symexec-bench.js --compare",
    "profile:symexec": "CALC_PROFILE=1 node benchmarks/engine/symexec-bench.js --profile"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@hpcc-js/wasm-graphviz": "^1.21.0",
    "@tailwindcss/typography": "^0.5.19",
    "cli-color": "^2.0.0",
    "highlight.js": "^11.11.1",
    "hono": "^4.11.9",
    "marked": "^17.0.3",
    "marked-highlight": "^2.2.3",
    "tree-sitter": "^0.25.0",
    "web-tree-sitter": "^0.25.10"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.1.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@solidjs/router": "^0.15.3",
    "@tailwindcss/vite": "^4.1.8",
    "autoprefixer": "^10.4.21",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.21",
    "monorepo": "github:mdx-js/mdx",
    "playwright": "^1.57.0",
    "postcss": "^8.5.4",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "solid-js": "^1.9.5",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-solid": "^2.11.6"
  }
}
