{
  "name": "calc",
  "version": "2.0.0",
  "description": "Linear logic proof calculus sandbox",
  "main": "index.js",
  "scripts": {
    "test": "node --test tests/test.js tests/node.js tests/proofstate.js tests/hash.js tests/store.js tests/term.js tests/intern.js tests/profiler.js && node tests/polarity-inference.js",
    "test:e2e": "node tests/e2e-solidjs.js",
    "test:all": "npm test && npm run test:e2e",
    "build:parser": "./libexec/calc-genparser",
    "dev": "npm run build:parser && vite --config src/ui/vite.config.ts",
    "build:ui": "npm run build:parser && vite build --config src/ui/vite.config.ts",
    "preview": "vite preview --config src/ui/vite.config.ts",
    "export": "./libexec/calc-export > src/html/calculus.html",
    "export:full": "./libexec/calc-export --full > src/html/calculus-full.html",
    "export:meta": "./libexec/calc-export --meta > src/html/calculus-meta.html",
    "bench": "node --expose-gc benchmarks/run.js",
    "bench:proof": "node --expose-gc benchmarks/run.js --category=proof",
    "bench:micro": "node --expose-gc benchmarks/run.js --category=micro",
    "bench:save": "node --expose-gc benchmarks/run.js --save",
    "bench:compare": "node --expose-gc benchmarks/run.js --compare",
    "profile": "CALC_PROFILE=1 node"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cli-color": "^2.0.0",
    "copy-paste": "^1.3.0",
    "jison": "^0.4.18",
    "jison-lex": "^0.3.4",
    "readline-sync": "^1.4.7",
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@solidjs/router": "^0.15.3",
    "@tailwindcss/vite": "^4.1.8",
    "autoprefixer": "^10.4.21",
    "katex": "^0.16.21",
    "playwright": "^1.57.0",
    "postcss": "^8.5.4",
    "solid-js": "^1.9.5",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-solid": "^2.11.6"
  }
}
