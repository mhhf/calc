{
  "name": "calc",
  "version": "1.0.0",
  "description": "Linear logic proof calculus sandbox",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha tests --ignore tests/e2e-ui.js",
    "test:e2e": "node tests/e2e-ui.js",
    "test:all": "npm test && npm run test:e2e",
    "build": "npx webpack",
    "build:parser": "./libexec/calc-genparser",
    "export": "./libexec/calc-export > src/html/calculus.html",
    "export:full": "./libexec/calc-export --full > src/html/calculus-full.html",
    "export:meta": "./libexec/calc-export --meta > src/html/calculus-meta.html",
    "serve": "python3 -m http.server 8080 --directory src/html",
    "ui": "npm run build && npm run serve"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cycle/dom": "^23.1.0",
    "@cycle/run": "^5.7.0",
    "cli-color": "^2.0.0",
    "copy-paste": "^1.3.0",
    "crypto-browserify": "^3.12.1",
    "jison": "^0.4.18",
    "jison-lex": "^0.3.4",
    "katex": "^0.8.2",
    "neodoc": "^1.4.0",
    "readline-sync": "^1.4.7",
    "shelljs": "^0.7.8",
    "stream-browserify": "^3.0.0",
    "viz.js": "^1.8.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "xstream": "^11.14.0"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "jsdom": "^27.4.0",
    "mocha": "^3.5.0",
    "playwright": "^1.57.0",
    "puppeteer": "^24.35.0"
  }
}
