{
  "name": "calc",
  "version": "2.0.0",
  "description": "Linear logic proof calculus sandbox",
  "main": "index.js",
  "scripts": {
    "test": "node --test tests/hash.js tests/tree-sitter.js tests/v2/*.test.js tests/rule-interpreter.test.js tests/manual-prover-logic.test.js tests/manual-proof-flow.test.js",
    "test:v2": "node --test tests/v2/calculus.test.js tests/v2/sequent.test.js tests/v2/kernel.test.js tests/v2/focusing.test.js tests/v2/sequent-ext.test.js tests/v2/focused-prover.test.js tests/v2/api.test.js",
    "test:mde": "node --test tests/mde/convert.test.js tests/mde/forward.test.js tests/mde/e2e.test.js",
    "test:celf": "echo 'celf tests removed'",
    "test:ts": "node --test tests/tree-sitter.js",
    "test:e2e": "node tests/e2e-solidjs.js",
    "test:all": "npm test && npm run test:mde",
    "build:ts": "cd lib/tree-sitter-mde && tree-sitter generate && tree-sitter build --output mde.so",
    "build:ts:wasm": "cd lib/tree-sitter-mde && tree-sitter build --wasm",
    "build:v2-bundle": "node libexec/calc-bundle-v2",
    "dev": "npm run build:v2-bundle && vinxi dev",
    "dev:vite": "npm run build:v2-bundle && vite --config src/ui/vite.config.ts",
    "build": "npm run build:v2-bundle && vinxi build",
    "start": "vinxi start",
    "build:ui": "npm run build:v2-bundle && vite build --config src/ui/vite.config.ts",
    "preview": "vite preview --config src/ui/vite.config.ts",
    "export": "./libexec/calc-export > src/html/calculus.html",
    "export:full": "./libexec/calc-export --full > src/html/calculus-full.html",
    "export:meta": "./libexec/calc-export --meta > src/html/calculus-meta.html",
    "bench": "node --expose-gc benchmarks/run.js",
    "bench:proof": "node --expose-gc benchmarks/run.js --category=proof",
    "bench:micro": "node --expose-gc benchmarks/run.js --category=micro",
    "bench:save": "node --expose-gc benchmarks/run.js --save",
    "bench:compare": "node --expose-gc benchmarks/run.js --compare",
    "bench:v2": "node --expose-gc benchmarks/proof/proofs-v2.bench.js",
    "bench:compare:all": "node --expose-gc benchmarks/compare-all.js",
    "bench:compare:all:save": "node --expose-gc benchmarks/compare-all.js --save",
    "bench:mde": "node --expose-gc benchmarks/mde/run.js",
    "bench:mde:save": "node --expose-gc benchmarks/mde/run.js --save",
    "bench:mde:compare": "node --expose-gc benchmarks/mde/run.js --compare",
    "bench:mde:stress": "node --expose-gc benchmarks/mde/run.js --stress",
    "profile": "CALC_PROFILE=1 node",
    "profile:mde": "node --expose-gc benchmarks/mde/run.js --profile",
    "profile:multisig": "CALC_PROFILE=1 node tests/mde/multisig-profile-deep.js",
    "bench:multisig": "node tests/mde/multisig-bench.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hpcc-js/wasm-graphviz": "^1.21.0",
    "cli-color": "^2.0.0",
    "copy-paste": "^1.3.0",
    "readline-sync": "^1.4.7",
    "shelljs": "^0.7.8",
    "tree-sitter": "^0.25.0",
    "web-tree-sitter": "^0.25.10"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.1.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.2.1",
    "@tailwindcss/vite": "^4.1.8",
    "autoprefixer": "^10.4.21",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.21",
    "monorepo": "github:mdx-js/mdx",
    "playwright": "^1.57.0",
    "postcss": "^8.5.4",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "solid-js": "^1.9.5",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vinxi": "^0.5.11",
    "vite": "^6.3.5",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-solid": "^2.11.6"
  }
}
