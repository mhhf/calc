%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%  00

evm/stop:
  pc PC *
  code PC (e) *
  !inc PC PC'
  -o {
    code PC (e) *
    stop
  }.

evm/add:
  pc PC *
  code PC (i (e)) *
  !inc PC PC' *
  sh (s (s SH)) *
  stack (s SH) A *
  stack SH  B *
  !plus A B C
  -o {
    code PC (i (e)) *
    pc PC' *
    sh (s SH) *
    stack SH C
  }.

evm/mul:
  pc PC *
  code PC (o (i (e))) *
  !inc PC PC' *
  sh (s (s SH)) *
  stack (s SH) A *
  stack SH  B *
  trim A A' *
  trim B B' *
  !mul A B C *
  to256 C C'
  -o {
    code PC (o (i (e))) *
    pc PC' *
    sh (s SH) *
    stack SH C'
  }.

evm/sub:
  pc PC *
  code PC (i (i (e))) *
  !inc PC PC' *
  sh (s (s SH)) *
  stack (s SH) A *
  stack SH  B *
  !plus C B A
  -o {
    code PC (i (i (e))) *
    pc PC' *
    sh (s SH) *
    stack SH C
  }.

evm/exp:
  pc PC *
  code PC (o (i (o (i (e))))) *
  !inc PC PC' *
  sh (s (s SH)) *
  stack (s SH) Base *
  stack SH  Exponent *
  !trim Base Base' *
  !trim Exponent Exponent' *
  !exp Base' Exponent' Result *
  !to256 Result Result'
  -o {
    debug Base' *
    debug Exponent' *
    code PC (o (i (o (i (e))))) *
    pc PC' *
    sh (s SH) *
    stack SH Result'
  }.

