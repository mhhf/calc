evm/eq:
  pc PC *
  code PC (o (o (i (o (i (e)))))) *
  !inc PC PC' *
  gas GAS *
  !plus (o(i e)) GAS GAS' * %10 = 2 *
  sh (s (s SH)) *
  stack (s SH) X *
  stack SH Y
  -o {
    pc PC' *
    gas GAS' *
    code PC (o (o (i (o (i (e)))))) *
    sh (s SH) *
    ((!neq X Y -o {
      stack SH (e)
    }) &
    (!eq X Y -o {
      stack SH (i (e))
    }))
  }.

evm/iszero_eq:
  pc PC *
  code PC (i (o (i (o (i (e)))))) *
  !inc PC PC' *
  gas GAS *
  !inc GAS GAS' *
  sh (s SH) *
  stack SH (e)
  -o {
    code PC (i (o (i (o (i (e)))))) *
    pc PC' *
    gas GAS' *
    sh (s SH) *
    stack SH (i (e))
  }.

evm/iszero_neq:
  pc PC *
  code PC (i (o (i (o (i (e)))))) *
  !inc PC PC' *
  gas GAS *
  !inc GAS GAS' *
  sh (s SH) *
  stack SH V *
  !neq V (e)
  -o {
    code PC (i (o (i (o (i (e)))))) *
    pc PC' *
    gas GAS' *
    sh (s SH) *
    stack SH (e)
  }.

evm/lt:
  pc PC *
  code PC (o (o (o (o (i (e)))))) *
  !inc PC PC' *
  sh (s (s SH)) *
  stack (s SH) X *
  stack SH Y *
  !gt Y X (e) Z *
  to256 Z Z'
  -o {
    code PC (o (o (o (o (i (e)))))) *
    pc PC' *
    sh (s SH) *
    stack SH Z'
  }.

evm/gt:
