concatMemory: bin -> bin -> bin -> type.
unblockConcatMemory: bin -> type.
concatMemory/z: concatMemory X X Z
  -o {
    unblockConcatMemory Z
  }.
concatMemory/s:
  concatMemory Pos To Z *
  !neq Pos To           *
  memory Pos Size V     *
  !plus Pos Size Pos'
  -o {
    memory Pos Size V *
    concatMemory Pos' To (concat Z V)
  }.

evm/sha3:
  pc PC             *
  code PC (o (o (o (o (o (i (e)))))))      *
  !inc PC PC'       *
  gas GAS           *
  !inc GAS GAS'     *
  sh (s (s SH))     *
  stack (s SH) From *
  stack SH  To
  -o {
    concatMemory From To e *
    (unblockConcatMemory Z -o {
      code PC (o (o (o (o (o (i (e))))))) *
      pc PC' *
      gas GAS' *
      stack SH (sha3 Z) *
      sh (s SH)
    })
  }.

