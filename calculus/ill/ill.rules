@formulas A, B, C

id: ; A |- A
  @pretty "Id"
  @invertible true.

tensor_r: G ; D, D' |- A * B
  <- G ; D |- A
  <- G ; D' |- B
  @pretty "⊗R".

tensor_l: G ; D, A * B |- C
  <- G ; D, A, B |- C
  @pretty "⊗L".

loli_r: G ; D |- A -o B
  <- G ; D, A |- B
  @pretty "⊸R".

loli_l: G ; D, D', A -o B |- C
  <- G ; D |- A
  <- G ; D', B |- C
  @pretty "⊸L".

with_r: G ; D |- A & B
  <- G ; D |- A
  <- G ; D |- B
  @pretty "&R".

with_l1: G ; D, A & B |- C
  <- G ; D, A |- C
  @pretty "&L₁".

with_l2: G ; D, A & B |- C
  <- G ; D, B |- C
  @pretty "&L₂".

oplus_r1: G ; D |- A + B
  <- G ; D |- A
  @pretty "+R₁".

oplus_r2: G ; D |- A + B
  <- G ; D |- B
  @pretty "+R₂".

oplus_l: G ; D, A + B |- C
  <- G ; D, A |- C
  <- G ; D, B |- C
  @pretty "+L".

exists_r: G ; D |- exists A
  <- G ; D |- A
  @pretty "∃R"
  @binding metavar.

exists_l: G ; D, exists A |- C
  <- G ; D, A |- C
  @pretty "∃L"
  @binding eigenvariable.

forall_r: G ; D |- forall A
  <- G ; D |- A
  @pretty "∀R"
  @binding eigenvariable.

forall_l: G ; D, forall A |- C
  <- G ; D, A |- C
  @pretty "∀L"
  @binding metavar.

one_r: G ; |- I
  @pretty "1R".

one_l: G ; D, I |- C
  <- G ; D |- C
  @pretty "1L".

promotion: G ; |- !A
  <- G ; |- A
  @pretty "!R"
  @invertible false
  @bridge linear_to_cartesian.

dereliction: G ; D, !A |- C
  <- G ; D, A |- C
  @pretty "!D"
  @invertible true.

absorption: G ; D, !A |- C
  <- G, A ; D |- C
  @pretty "!L"
  @invertible true
  @bridge linear_to_cartesian.

copy: G, A ; D |- C
  <- G, A ; D, A |- C
  @pretty "Copy"
  @structural true.
